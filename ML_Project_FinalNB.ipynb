    "import os\n",
    "import os.path\n",
    "import cv2\n",
    "import glob\n",
    "import imutils\n",
    "\n",
    "\n",
    "CAPTCHA_IMAGE_FOLDER = \"generated_captcha_images\"\n",
    "OUTPUT_FOLDER = \"extracted_letter_images\"\n",
    "\n",
    "# Get all captcha images\n",
    "capt_images = glob.glob(os.path.join(CAPTCHA_IMAGE_FOLDER, \"*\"))\n",
    "counts = {}\n",
    "for (i, capt_img) in enumerate(capt_images):\n",
    "    print(\"Processing image {}/{}\".format(i + 1, len(capt_images)))\n",
    "    filename = os.path.basename(capt_img)\n",
    "    capt_text = os.path.splitext(filename)[0]\n",
    "\n",
    "    #converting to grayscale\n",
    "    image = cv2.imread(capt_img)\n",
    "    gray = cv2.cvtColor(image, cv2.COLOR_BGR2GRAY)\n",
    "\n",
    "    # Adding extra padding\n",
    "    gray = cv2.copyMakeBorder(gray, 8, 8, 8, 8, cv2.BORDER_REPLICATE)\n",
    "\n",
    "    # converting to complete black and white\n",
    "    thresh = cv2.threshold(gray, 0, 255, cv2.THRESH_BINARY_INV | cv2.THRESH_OTSU)[1]\n",
    "\n",
    "    # finding the contours\n",
    "    contours = cv2.findContours(thresh.copy(), cv2.RETR_EXTERNAL, cv2.CHAIN_APPROX_SIMPLE)\n",
    "    contours = contours[0] if imutils.is_cv2() else contours[1]\n",
    "\n",
    "    letter_imgs = []\n",
    "\n",
    "    #extracting the letter\n",
    "    for contour in contours:\n",
    "        (x, y, w, h) = cv2.boundingRect(contour)\n",
    "        # Compare width and height of the contour to find overlapping letters\n",
    "        if w / h > 1.25:\n",
    "            #Split the contour into half if it is too wide\n",
    "            half_contour = int(w / 2)\n",
    "            letter_imgs.append((x, y, half_contour, h))\n",
    "            letter_imgs.append((x + half_contour, y, half_contour, h))\n",
    "        else:\n",
    "            letter_imgs.append((x, y, w, h))\n",
    "            \n",
    "    if len(letter_imgs) != 4:\n",
    "        continue\n",
    "\n",
    "    #Sorting the letter images\n",
    "    letter_imgs = sorted(letter_imgs, key=lambda x: x[0])\n",
    "\n",
    "    #saving each letter as single image\n",
    "    for letter_rect, letter_text in zip(letter_imgs, capt_text):\n",
    "        x, y, w, h = letter_rect\n",
    "        letter_image = gray[y - 2:y + h + 2, x - 2:x + w + 2]\n",
    "        path = os.path.join(OUTPUT_FOLDER, letter_text)\n",
    "        if not os.path.exists(path):\n",
    "            os.makedirs(path)\n",
    "        count = counts.get(letter_text, 1)\n",
    "        p = os.path.join(path, \"{}.png\".format(str(count).zfill(6)))\n",
    "        cv2.imwrite(p, letter_image)\n",
    "        counts[letter_text] = count + 1\n"
   ]
  },
